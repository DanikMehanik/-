applicationName: $APP_NAME
applicationDomain: $APP_DOMAIN
applicationURL: $APP_NAME-$DEPLOY_REF_SLUG.$APP_DOMAIN
applicationSlug: $DEPLOY_REF_SLUG


registry:
    url: $CI_REGISTRY
    username: $CI_DEPLOY_USER
    password: $CI_DEPLOY_PASSWORD

db:
    app:
        host: $DB_APP_HOST
        port: $DB_APP_PORT
        database: $DB_APP_DATABASE
        user: $DB_APP_USER
        password: $DB_APP_PASSWORD


image:
    dashboard: $CI_REGISTRY_IMAGE/dashboard:$DEPLOY_REF_SLUG
